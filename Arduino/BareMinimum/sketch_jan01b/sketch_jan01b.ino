int a=13;
int b=12;
int c=11;
int d=10;
int d1=1000;


void setup() {
pinMode(a,OUTPUT);
pinMode(b,OUTPUT);
pinMode(c,OUTPUT);
pinMode(d,OUTPUT);
}

void loop() {
